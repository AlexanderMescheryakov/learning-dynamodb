{
  "name": "learning-dynamodb",
  "version": "1.0.0",
  "description": "Learning DynamoDB",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --fix",
    "cdk": "cdk",
    "test": "jest",
    "dynamodb-import": "dynamodb-import"
  },
  "license": "Noncommercial",
  "devDependencies": {
    "@aws-cdk/assert": "1.90.0",
    "@tsconfig/node12": "1.0.7",
    "@types/jest": "26.0.19",
    "@types/node": "12.19.8",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "cdk": "1.90.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudwatch-actions": "1.90.0",
    "@aws-cdk/aws-cognito": "1.90.0",
    "@aws-cdk/aws-dynamodb": "1.90.0",
    "@aws-cdk/aws-events-targets": "1.90.0",
    "@aws-cdk/aws-lambda": "1.90.0",
    "@aws-cdk/aws-lambda-event-sources": "1.90.0",
    "@aws-cdk/aws-s3": "1.90.0",
    "@aws-cdk/aws-s3-assets": "1.90.0",
    "@aws-cdk/aws-s3-deployment": "1.90.0",
    "@aws-cdk/aws-sns-subscriptions": "1.90.0",
    "@aws-cdk/core": "1.90.0",
    "dynamodb-import": "1.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ]
  }
}
